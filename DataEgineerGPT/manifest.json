{
  "manifest_version": 1,
  "name": "DataEgineerGPT",
  "version": "1.0",
  "description": "Useful Chrome plugin for Data Engineering",
  "permissions": ["activeTab", "scripting"],
  "icons": {

  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
